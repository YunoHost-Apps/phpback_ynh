{
    "name": "PHPBack",
    "id": "phpback",
    "packaging_format": 1,
    "description": {
        "en": "Open source feedback system you can use for your website",
        "fr": "Open source feedback system you can use for your website"
    },
    "version": "1.3.2~ynh1",
    "url": "https://www.phpback.org/",
    "upstream": {
        "license": "AGPL-3.0-only",
        "website": "https://www.phpback.org/",
        "demo": "https://www.phpback.org/demo/",
        "code": "https://github.com/phpback/phpback"
    },
    "license": "AGPL-3.0-only",
    "maintainer": {
        "name": "eric_G",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 11.2"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.4-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/feedback",
                "default": "/feedback"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
